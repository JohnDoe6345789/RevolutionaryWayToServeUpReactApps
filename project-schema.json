{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Revolutionary Project Codegen Specification",
  "description": "Complete project structure and code generation specification",
  "type": "object",
  "properties": {
    "project": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier (auto-generated UUID if not provided)",
          "default": "${function:generateId}"
        },
        "searchTags": {
          "type": "array",
          "description": "Tags for search ranking boosts",
          "items": { "type": "string" },
          "default": []
        },
        "searchBoost": {
          "type": "number",
          "description": "Search priority boost multiplier",
          "default": 1.0
        },
        "name": {
          "type": "string",
          "description": "Project name",
          "pattern": "^[a-zA-Z][a-zA-Z0-9-_]*$"
        },
        "version": {
          "type": "string",
          "description": "Project version",
          "pattern": "^\\d+\\.\\d+\\.\\d+$"
        },
        "description": {
          "type": "string",
          "description": "Project description"
        },
        "author": {
          "type": "string",
          "description": "Project author"
        },
        "license": {
          "type": "string",
          "description": "Project license",
          "default": "MIT"
        },
        "repository": {
          "type": "string",
          "description": "Git repository URL"
        },
        "generated": {
          "type": "string",
          "description": "Generation timestamp",
          "default": "${function:getTimestamp}"
        }
      },
      "required": ["name", "version", "description"]
    },
    "structure": {
      "type": "object",
      "description": "Project folder and file structure",
      "properties": {
        "rootDir": {
          "type": "string",
          "description": "Root directory for generated project",
          "default": "./generated-project"
        },
        "folders": {
          "type": "array",
          "description": "Folder structure",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Folder name"
              },
              "path": {
                "type": "string",
                "description": "Relative path from root"
              },
              "description": {
                "type": "string",
                "description": "Folder purpose"
              },
              "readmeContent": {
                "type": "string",
                "description": "README.md content for this folder"
              },
              "subfolders": {
                "type": "array",
                "description": "Nested folders",
                "items": { "$ref": "#/properties/structure/properties/folders/items" }
              }
            },
            "required": ["name", "path"]
          }
        },
        "files": {
          "type": "array",
          "description": "Static files to create",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "File name"
              },
              "path": {
                "type": "string",
                "description": "File path"
              },
              "template": {
                "type": "string",
                "description": "Template to use"
              },
              "content": {
                "type": "string",
                "description": "File content"
              },
              "encoding": {
                "type": "string",
                "description": "File encoding",
                "default": "utf8"
              }
            },
            "required": ["name", "path"]
          }
        }
      },
      "required": ["rootDir", "folders"]
    },
    "classes": {
      "type": "object",
      "description": "Class generation specifications",
      "properties": {
        "businessLogic": {
          "type": "array",
          "description": "Business logic classes with initialize/execute pattern",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Class name"
              },
              "description": {
                "type": "string",
                "description": "Class purpose"
              },
              "extends": {
                "type": "string",
                "description": "Base class to extend",
                "default": "BaseClass"
              },
              "implements": {
                "type": "array",
                "description": "Interfaces to implement",
                "items": { "type": "string" }
              },
              "module": {
                "type": "string",
                "description": "Module path"
              },
              "dataClass": {
                "type": "string",
                "description": "Data class name",
                "default": "${name}Data"
              },
              "factory": {
                "type": "string",
                "description": "Factory class name",
                "default": "${name}Factory"
              },
              "config": {
                "type": "object",
                "description": "Class configuration"
              },
              "initializeLogic": {
                "type": "string",
                "description": "Initialize method implementation"
              },
              "executeLogic": {
                "type": "string",
                "description": "Execute method implementation"
              },
              "dependencies": {
                "type": "array",
                "description": "Class dependencies",
                "items": { "type": "string" }
              }
            },
            "required": ["name", "description", "module"]
          }
        },
        "aggregates": {
          "type": "array",
          "description": "Aggregate classes with unlimited nesting",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Aggregate name"
              },
              "description": {
                "type": "string",
                "description": "Aggregate purpose"
              },
              "parent": {
                "type": "string",
                "description": "Parent aggregate name"
              },
              "nestingLevel": {
                "type": "integer",
                "description": "Nesting level",
                "minimum": 0
              },
              "factory": {
                "type": "string",
                "description": "Factory class name"
              },
              "dataClass": {
                "type": "string",
                "description": "Data class name"
              },
              "module": {
                "type": "string",
                "description": "Module path"
              },
              "children": {
                "type": "array",
                "description": "Child aggregates",
                "items": { "$ref": "#/properties/classes/properties/aggregates/items" }
              },
              "getMethods": {
                "type": "array",
                "description": "Auto-generated get methods",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Method name"
                    },
                    "target": {
                      "type": "string",
                      "description": "Target aggregate"
                    },
                    "path": {
                      "type": "string",
                      "description": "Method path"
                    }
                  },
                  "required": ["name", "target"]
                }
              },
              "config": {
                "type": "object",
                "description": "Aggregate configuration"
              }
            },
            "required": ["name", "description", "module"]
          }
        },
        "factories": {
          "type": "array",
          "description": "Factory class specifications",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Factory name"
              },
              "targetClass": {
                "type": "string",
                "description": "Target class to create"
              },
              "dataClass": {
                "type": "string",
                "description": "Data class to use"
              },
              "module": {
                "type": "string",
                "description": "Target module path"
              },
              "config": {
                "type": "object",
                "description": "Factory configuration"
              },
              "validationRules": {
                "type": "array",
                "description": "Validation rules",
                "items": {
                  "type": "object",
                  "properties": {
                    "property": {
                      "type": "string",
                      "description": "Property to validate"
                    },
                    "rule": {
                      "type": "string",
                      "description": "Validation rule"
                    },
                    "message": {
                      "type": "string",
                      "description": "Error message"
                    }
                  },
                  "required": ["property", "rule", "message"]
                }
              }
            },
            "required": ["name", "targetClass", "dataClass", "module"]
          }
        },
        "dataClasses": {
          "type": "array",
          "description": "Data class specifications",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Data class name"
              },
              "description": {
                "type": "string",
                "description": "Data class purpose"
              },
              "extends": {
                "type": "string",
                "description": "Base data class",
                "default": "BaseData"
              },
              "properties": {
                "type": "array",
                "description": "Data properties",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Property name"
                    },
                    "type": {
                      "type": "string",
                      "enum": ["string", "number", "boolean", "array", "object", "any"],
                      "description": "Property type"
                    },
                    "required": {
                      "type": "boolean",
                      "description": "Whether property is required",
                      "default": true
                    },
                    "default": {
                      "description": "Default value"
                    },
                    "validation": {
                      "type": "object",
                      "description": "Validation rules"
                    }
                  },
                  "required": ["name", "type"]
                }
              },
              "validation": {
                "type": "object",
                "description": "Class-level validation"
              }
            },
            "required": ["name", "description", "properties"]
          }
        }
      },
      "required": ["businessLogic", "aggregates", "factories", "dataClasses"]
    },
    "plugins": {
      "type": "object",
      "description": "Plugin system configuration",
      "properties": {
        "groups": {
          "type": "array",
          "description": "Plugin groups",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Group name"
              },
              "description": {
                "type": "string",
                "description": "Group purpose"
              },
              "category": {
                "type": "string",
                "enum": ["analysis", "generation", "utility", "codegen"],
                "description": "Group category"
              },
              "plugins": {
                "type": "array",
                "description": "Plugins in this group",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Unique identifier (auto-generated UUID if not provided)",
                      "default": "${function:generateId}"
                    },
                    "searchTags": {
                      "type": "array",
                      "description": "Tags for search ranking boosts",
                      "items": { "type": "string" },
                      "default": []
                    },
                    "searchBoost": {
                      "type": "number",
                      "description": "Search priority boost multiplier",
                      "default": 1.0
                    },
                    "name": {
                      "type": "string",
                      "description": "Plugin name"
                    },
                    "description": {
                      "type": "string",
                      "description": "Plugin purpose"
                    },
                    "version": {
                      "type": "string",
                      "description": "Plugin version"
                    },
                    "author": {
                      "type": "string",
                      "description": "Plugin author"
                    },
                    "dependencies": {
                      "type": "array",
                      "description": "Plugin dependencies",
                      "items": { "type": "string" }
                    },
                    "config": {
                      "type": "object",
                      "description": "Plugin configuration"
                    }
                  },
                  "required": ["name", "description", "version"]
                }
              }
            },
            "required": ["name", "description", "category"]
          }
        },
        "codegen": {
          "type": "object",
          "description": "Codegen plugin configuration",
          "properties": {
            "id": {
              "type": "string",
              "description": "Unique identifier (auto-generated UUID if not provided)",
              "default": "${function:generateId}"
            },
            "searchTags": {
              "type": "array",
              "description": "Tags for search ranking boosts",
              "items": { "type": "string" },
              "default": []
            },
            "searchBoost": {
              "type": "number",
              "description": "Search priority boost multiplier",
              "default": 1.0
            },
            "name": {
              "type": "string",
              "description": "Codegen plugin name",
              "default": "RevolutionaryCodegen"
            },
            "description": {
              "type": "string",
              "description": "Codegen plugin purpose"
            },
            "generators": {
              "type": "array",
              "description": "Available generators",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "description": "Unique identifier (auto-generated UUID if not provided)",
                    "default": "${function:generateId}"
                  },
                  "searchTags": {
                    "type": "array",
                    "description": "Tags for search ranking boosts",
                    "items": { "type": "string" },
                    "default": []
                  },
                  "searchBoost": {
                    "type": "number",
                    "description": "Search priority boost multiplier",
                    "default": 1.0
                  },
                  "name": {
                    "type": "string",
                    "description": "Generator name"
                  },
                  "description": {
                    "type": "string",
                    "description": "Generator purpose"
                  },
                  "class": {
                    "type": "string",
                    "description": "Generator class"
                  },
                  "templates": {
                    "type": "object",
                    "description": "Generator templates"
                  }
                },
                "required": ["name", "description", "class"]
              }
            }
          }
        }
      },
      "required": ["groups"]
    },
    "codegen": {
      "type": "object",
      "description": "Code generation configuration",
      "properties": {
        "templates": {
          "type": "object",
          "description": "Code generation templates",
          "properties": {
            "businessClass": {
              "type": "string",
              "description": "Business logic class template"
            },
            "dataClass": {
              "type": "string",
              "description": "Data class template"
            },
            "factoryClass": {
              "type": "string",
              "description": "Factory class template"
            },
            "aggregateClass": {
              "type": "string",
              "description": "Aggregate class template"
            }
          }
        },
        "rules": {
          "type": "object",
          "description": "Code generation rules",
          "properties": {
            "enforceTwoMethodLimit": {
              "type": "boolean",
              "description": "Enforce initialize/execute only",
              "default": true
            },
            "requireDataclassPattern": {
              "type": "boolean",
              "description": "Require dataclass constructor pattern",
              "default": true
            },
            "requireSingleConstructorParam": {
              "type": "boolean",
              "description": "Require single constructor parameter",
              "default": true
            },
            "requireBaseClassInheritance": {
              "type": "boolean",
              "description": "Require base class inheritance",
              "default": true
            },
            "maxClassLines": {
              "type": "integer",
              "description": "Maximum class lines",
              "default": 100
            },
            "maxMethodLines": {
              "type": "integer",
              "description": "Maximum method lines",
              "default": 20
            }
          }
        },
        "validation": {
          "type": "object",
          "description": "Validation configuration",
          "properties": {
            "strictMode": {
              "type": "boolean",
              "description": "Enable strict validation",
              "default": true
            },
            "autoFix": {
              "type": "boolean",
              "description": "Enable auto-fix for common issues",
              "default": false
            },
            "warningsAsErrors": {
              "type": "boolean",
              "description": "Treat warnings as errors",
              "default": false
            }
          }
        },
        "output": {
          "type": "object",
          "description": "Output configuration",
          "properties": {
            "cleanBeforeGenerate": {
              "type": "boolean",
              "description": "Clean output directory before generation",
              "default": true
            },
            "backupExisting": {
              "type": "boolean",
              "description": "Backup existing files",
              "default": true
            },
            "generateTypeScript": {
              "type": "boolean",
              "description": "Generate TypeScript definitions",
              "default": true
            },
            "generateTests": {
              "type": "boolean",
              "description": "Generate test stubs",
              "default": true
            },
            "generateDocs": {
              "type": "boolean",
              "description": "Generate documentation",
              "default": true
            }
          }
        }
      },
      "required": ["rules", "validation", "output"]
    },
    "constants": {
      "type": "object",
      "description": "Project constants and functions",
      "properties": {
        "maxNestingLevel": {
          "type": "integer",
          "description": "Maximum aggregate nesting level",
          "default": 5
        },
        "defaultAggregateType": {
          "type": "string",
          "description": "Default aggregate type",
          "default": "nested"
        },
        "generatedCodeMarker": {
          "type": "string",
          "description": "Marker for generated code",
          "default": "Auto-generated by RevolutionaryCodegen"
        },
        "definitions": {
          "type": "array",
          "description": "Project constant definitions",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Constant name"
              },
              "value": {
                "description": "Constant value (any type)"
              },
              "type": {
                "type": "string",
                "enum": ["string", "number", "boolean", "object", "array"],
                "description": "Constant type"
              },
              "description": {
                "type": "string",
                "description": "Constant description"
              },
              "category": {
                "type": "string",
                "description": "Constant category for organization"
              }
            },
            "required": ["name", "value", "type"]
          }
        },
        "functions": {
          "type": "object",
          "description": "JavaScript functions for template processing",
          "properties": {
            "getTimestamp": {
              "type": "string",
              "description": "Current timestamp function",
              "default": "return new Date().toISOString();"
            },
            "calculateVersion": {
              "type": "string",
              "description": "Version calculation function",
              "default": "return Math.floor(Date.now() / 1000).toString();"
            },
            "generateId": {
              "type": "string",
              "description": "ID generation function",
              "default": "return Date.now().toString(36) + Math.random().toString(36).substr(2);"
            }
          }
        }
      }
    },
    "environment": {
      "type": "object",
      "description": "Environment variable configuration",
      "properties": {
        "variables": {
          "type": "array",
          "description": "Environment variable definitions",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier (auto-generated UUID if not provided)",
                "default": "${function:generateId}"
              },
              "searchTags": {
                "type": "array",
                "description": "Tags for search ranking boosts",
                "items": { "type": "string" },
                "default": []
              },
              "searchBoost": {
                "type": "number",
                "description": "Search priority boost multiplier",
                "default": 1.0
              },
              "name": {
                "type": "string",
                "description": "Environment variable name"
              },
              "description": {
                "type": "string",
                "description": "Variable description"
              },
              "required": {
                "type": "boolean",
                "description": "Whether this variable is required",
                "default": false
              },
              "default": {
                "description": "Default value if not provided"
              },
              "type": {
                "type": "string",
                "enum": ["string", "number", "boolean", "json"],
                "description": "Variable type",
                "default": "string"
              },
              "validation": {
                "type": "object",
                "description": "Validation rules",
                "properties": {
                  "pattern": {
                    "type": "string",
                    "description": "Regex pattern for validation"
                  },
                  "min": {
                    "type": "number",
                    "description": "Minimum value for numbers"
                  },
                  "max": {
                    "type": "number",
                    "description": "Maximum value for numbers"
                  },
                  "enum": {
                    "type": "array",
                    "description": "Allowed values"
                  }
                }
              }
            },
            "required": ["name", "description"]
          }
        },
        "configFile": {
          "type": "string",
          "description": "Environment configuration file name",
          "default": ".env"
        }
      }
    },
    "documentation": {
      "type": "object",
      "description": "Documentation generation configuration",
      "properties": {
        "generateDocstrings": {
          "type": "boolean",
          "description": "Generate docstrings for classes and methods",
          "default": true
        },
        "generateMarkdown": {
          "type": "boolean",
          "description": "Generate Markdown documentation",
          "default": true
        },
        "generateReadme": {
          "type": "boolean",
          "description": "Generate README.md in every folder",
          "default": true
        },
        "readmeTemplate": {
          "type": "string",
          "description": "Default README template"
        },
        "classDocumentation": {
          "type": "object",
          "description": "Class documentation settings",
          "properties": {
            "includeInheritance": {
              "type": "boolean",
              "description": "Include inheritance information",
              "default": true
            },
            "includeDependencies": {
              "type": "boolean",
              "description": "Include dependency information",
              "default": true
            },
            "includeExamples": {
              "type": "boolean",
              "description": "Include usage examples",
