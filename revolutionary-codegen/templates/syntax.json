{
  "javascript": {
    "errorHandling": {
      "throwError": "throw new Error('{message}');",
      "throwErrorWithCode": "throw new Error('{message}');"
    },
    "logging": {
      "log": "console.log('{message}'{levelOption});",
      "logWithLevel": "console.log('[{timestamp}] [{context}] {prefix} {message}');"
    },
    "moduleExport": {
      "class": "module.exports = {className};",
      "multiple": "module.exports = { {exports} };",
      "default": "export default {className};"
    },
    "classDeclaration": {
      "basic": "class {className} {",
      "extends": "class {className} extends {baseClass} {",
      "constructor": "  constructor({params}) {",
      "method": "  {async }{methodName}({params}) {",
      "property": "  {propertyName};"
    },
    "comments": {
      "single": "// {comment}",
      "multi": "/*\n * {comment}\n */",
      "jsdoc": "/**\n * {comment}\n */"
    },
    "dataStructures": {
      "map": "new Map()",
      "set": "new Set()",
      "object": "{ {key}: {value} }"
    },
    "controlFlow": {
      "if": "if ({condition}) {",
      "else": "} else {",
      "for": "for (const {item} of {collection}) {",
      "tryCatch": "try {",
      "catch": "} catch ({errorVar}) {"
    }
  },
